{
  "version": "1.0.0-*",
  "title": "FlashCache",
  "description": "a fixed size high performant in memory cache",
  "language": "en-US",
  "authors": [ "Dorus Verhoeckx" ],
  "copyright": "Copyright Â© Dorus Verhoeckx 2017",
  "packOptions": {
    "projectUrl": "https://github.com/Ruzzie/flashcache",
    "licenseUrl": "https://github.com/Ruzzie/flashcache/blob/master/LICENSE.md",
    "repository": {
      "type": "git",
      "url": "git://github.com/Ruzzie/flashcache"
    }
  },
  "dependencies": {
    "Ruzzie.Common": "1.0.51"
  },
  "frameworks": {
    "netstandard1.6": {
      "dependencies": {
        "NETStandard.Library": "1.6.1",
        "System.Threading": "4.3.0",
        "System.Reflection": "4.3.0",
        "System.Reflection.Extensions": "4.3.0",
        "System.Reflection.TypeExtensions": "4.3.0",
        "System.Threading.Tasks.Parallel": "4.0.1"
      }
    },
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.1",
          "type": "platform"
        },
        "System.Threading": "4.3.0",
        "System.Reflection": "4.3.0",
        "System.Reflection.Extensions": "4.3.0",
        "System.Reflection.TypeExtensions": "4.3.0",
        "System.Threading.Tasks.Parallel": "4.0.1"
      }
    },
    "net45": {
      "buildOptions": {
        "define": [ "HAVE_SERIALIZABLE", "NET45", "HAVE_FULL_REFLECTION" ]
      },
      "frameworkAssemblies": {
      }
    },
    "net46": {
      "buildOptions": {
        "define": [ "HAVE_SERIALIZABLE", "HAVE_FULL_REFLECTION" ]
      },
      "frameworkAssemblies": {
      }
    },
    "net461": {
      "buildOptions": {
        "define": [ "HAVE_SERIALIZABLE", "HAVE_FULL_REFLECTION" ]
      },
      "frameworkAssemblies": {
      }

    },
    ".NETPortable,Version=v4.0,Profile=Profile344": {
      "buildOptions": {
        "define": [ "PORTABLE", "HAVE_FULL_REFLECTION" ]
      },
      "dependencies": {
        "Microsoft.Bcl.Async": "1.0.168"
      },
      "frameworkAssemblies": {
        "Microsoft.CSharp": "2.0.5.0",
        "mscorlib": "2.0.5.0",
        "System": "2.0.5.0",
        "System.Core": "2.0.5.0"
      }
    }
  },
  "buildOptions": {
    "xmlDoc": true,
    "optimize": true
  }
}
